{
  "parser": "babel-eslint",
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "env": {
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "no-anonymous-exports-page-templates": "off",
    "limited-exports-page-templates": "off",
    "react/no-unescaped-entities": "off"
  }
}

